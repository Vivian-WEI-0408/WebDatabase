    class Meta:
        model = Partrputable
        fields = '__all__'

class ParttableSerializer(serializers.ModelSerializer):
    class Meta:
        model = Parttable
        fields = '__all__'

class PlasmidneedSerializer(serializers.ModelSerializer):
    class Meta:
        model = Plasmidneed
        fields = '__all__'

class TbBackboneUserfileaddressSerializer(serializers.ModelSerializer):
    class Meta:
        model = TbBackboneUserfileaddress
        fields = '__all__'

class TbPartUserfileaddressSerializer(serializers.ModelSerializer):
    class Meta:
        model = TbPartUserfileaddress
        fields = '__all__'

class TbPlasmidUserfileaddressSerializer(serializers.ModelSerializer):
    class Meta:
        model = TbPlasmidUserfileaddress
        fields = '__all__'

