from rest_framework import serializers
from models import Backbonetable,Parentplasmidtable,Partrputable,Parttable,Plasmidneed,Straintable,TbBackboneUserfileaddress,TbPartUserfileaddress,TbPlasmidUserfileaddress,Testdatatable

class StraintableSerializer(serializers.ModelSerializer):
    class Meta:
        model = Straintable
        fields = '__all__'

